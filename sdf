@echo off
setlocal enabledelayedexpansion

set "ips=147.182.196.248 167.172.23.138 167.172.18.128 167.172.246.60 167.99.179.127 167.99.184.229 167.99.189.66 167.99.182.246 167.99.191.58 164.92.77.195 155.138.222.47 96.30.197.200 66.42.86.147 45.32.216.200 104.156.254.251"

for %%i in (%ips%) do (
    start wt new-tab --title "SSH %%i" cmd /k "ssh -t root@%%i"
)
